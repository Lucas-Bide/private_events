<%= render 'users/session_banner', locals: { login: false, signup: true } %>
<% if @message %>
  <div><%= @message %></div>
<% end %>
<%= form_with method: 'post', url: '/logins', local: true do |f| %>
  <%= label_tag :username, 'username' %>
  <%= text_field_tag :username %><br/><br/>
  <%= f.submit 'Log In' %>
<% end %>